{
    "manifest_version": 3,
    "name": "mamba-force",
    "description": "Web browser extensions running on anaconda.org for Mamba users",
    "version": "0.0.1",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "host_permissions":[
      "*://anaconda.org/*"
    ],
    "content_scripts": [
      {
        "matches": ["*://anaconda.org/*/*"],
        "js": ["index.js"]
      }
    ]
  }
  